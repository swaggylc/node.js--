<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% var school='东华理工大学'
    var list=[
    {
        no:1,
        name:'《万紫千红中国年》',
        people:'凤凰传奇'
    },{
        no:2,
        name:'魔术',
        people:'李宁'
    },{
        no:3,
        name:'小品《真假老师》',
        people:'贾玲'
    }]
    %>
    <center>
        <h1>
            <%=school%>
        </h1>
        <table border="1">
            <%for(var i=0;i < list.length;i++){%>
                <tr>
                <th><%=list[i].no %></th>
                <th><%=list[i].name %></th>
                <th><%=list[i].people %></th>
            </tr>
            <%}%>
            
        </table>
    </center>
</body>
</html>